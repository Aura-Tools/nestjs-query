{
  "name": "nestjs-query",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "clean": "lerna run clean",
    "compile": "lerna run compile",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "jest": "jest --runInBand --coverage",
    "jest:e2e": "jest --runInBand --config=./jest.e2e.config.js",
    "jest:unit": "jest --coverage --config=./jest.unit.config.js",
    "lerna:pub": "lerna publish from-git",
    "lerna:version": "lerna version",
    "lint": "eslint --cache --ext=.ts .",
    "lint:fix": "eslint --fix --ext=.ts .",
    "lint:no-cache": "eslint --ext=.ts .",
    "prepare": "husky install",
    "release": "npm run build && npm run lerna:version && npm run lerna:pub",
    "test": "npm run lint && npm run jest",
    "test:e2e": "npm run jest:e2e"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@apollo/gateway": "0.42.0",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-angular": "15.0.0",
    "@nestjs/common": "8.4.0",
    "@nestjs/core": "8.4.0",
    "@nestjs/graphql": "9.0.6",
    "@nestjs/testing": "8.4.0",
    "@nrwl/eslint-plugin-nx": "14.1.4",
    "@nrwl/jest": "14.1.4",
    "@nrwl/js": "14.1.4",
    "@nrwl/linter": "14.1.4",
    "@nrwl/node": "^14.1.4",
    "@nrwl/nx-cloud": "latest",
    "@types/graphql": "14.5.0",
    "@types/graphql-fields": "1.3.4",
    "@types/jest": "27.0.2",
    "@types/lodash.merge": "4.6.6",
    "@types/lodash.omit": "4.5.6",
    "@types/node": "16.11.7",
    "@types/pluralize": "0.0.29",
    "@types/ws": "8.2.0",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "coveralls": "3.1.1",
    "dataloader": "2.0.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "graphql": "15.7.2",
    "graphql-subscriptions": "1.2.1",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-extended": "0.11.5",
    "jest-snapshot-serializer-raw": "1.2.0",
    "lerna": "4.0.0",
    "nx": "14.1.4",
    "prettier": "2.4.1",
    "ts-jest": "27.0.7",
    "ts-mockito": "2.6.1",
    "ts-morph": "12.2.0",
    "ts-node": "9.1.1",
    "tsconfig-extends": "1.0.1",
    "typescript": "4.4.4"
  }
}
