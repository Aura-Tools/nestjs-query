{
  "tagVersionPrefix": "v",
  "packages": [
    "examples/*",
    "examples/federation/*",
    "packages/*"
  ],
  "command": {
    "version": {
      "message": "chore(release): publish %s",
      "allowBranch": "master",
      "exact": true,
      "createRelease": "github",
      "conventionalCommits": true
    },
    "bootstrap": {
      "hoist": true
    }
  },
  "version": "0.13.0"
}
